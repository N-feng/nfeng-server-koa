{
  "name": "koa-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "dev92": "cross-env NODE_ENV=92 ./node_modules/.bin/nodemon bin/www",
    "dev94": "cross-env NODE_ENV=94 ./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pm2": "pm2 start ./bin/processes.json",
    "pm2dev": "pm2 start ./bin/processes.json --env dev",
    "pm2dev94": "pm2 start ./bin/processes.json --env dev94",
    "pm2dev92": "pm2 start ./bin/processes.json --env dev92"
  },
  "dependencies": {
    "co": "^4.6.0",
    "cos-nodejs-sdk-v5": "^2.5.6",
    "debug": "^2.6.3",
    "dotenv": "^6.1.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^2.5.0",
    "koa-bouncer": "^6.0.4",
    "koa-json": "^1.1.3",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^1.5.2",
    "koa-views": "^3.1.0",
    "log4js": "^3.0.6",
    "module-alias": "^2.1.0",
    "mongoose": "^5.3.12",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "nodemon": "^1.8.1"
  }
}
